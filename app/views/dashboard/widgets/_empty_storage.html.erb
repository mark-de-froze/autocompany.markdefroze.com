<div id="EmptyStorage">
  <h2>Parts and Oils that end <span class="h6 text-muted">(less than 5)</span></h2>

  <table class="table">
    <thead>
    <tr>
      <th>Code</th>
      <th>Title</th>
      <th>Quantity</th>
    </tr>
    </thead>
    <tbody>
    <% [@oils,@parts].each do |storage| %>
      <% storage.each do |item| %>
        <tr>
          <td><%=item.code%></td>
          <td><%=item.title%></td>
          <td><%=item.quantity%> <%=item.unit_text%></td>
        </tr>
      <% end %>
    <% end %>
    </tbody>
  </table>
</div>